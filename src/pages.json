{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uv-(.*)": "@climblee/uv-ui/components/uv-$1/uv-$1.vue",
      "^Pro(.*)": "@/components/Pro$1/Pro$1.vue",
      "^Pro(.*)": "@/components/Business/Pro$1/Pro$1.vue",
      "^Pro(.*)": "@/components/Pro/Pro$1/Pro$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "pages": [
    // pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/portal/index"
    },
    {
      "path": "pages/login/index",
      "style": {
        "disableScroll": true
      }
    },
    {
      "path": "pages/login/mobileLogin",
      "style": {
        "disableScroll": true
      }
    }
  ],
  "subPackages": [
    {
      "root": "pagesTransfer",
      "name": "pagesTransfer",
      "pages": [
        {
          "path": "index"
        }
      ]
    }
  ],
  "preloadRule": {},
  "globalStyle": {
    "navigationStyle": "custom"
  }
}
